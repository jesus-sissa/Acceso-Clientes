<%@ Page Language="vb" AutoEventWireup="false" Theme="NegroAmarillo" MasterPageFile="~/MasterPage.Master"
    CodeBehind="Login.aspx.vb" Inherits="SissaPortal.Login" %>

<%@ Register Src="../webControls/Alerta.ascx" TagName="Alerta" TagPrefix="uc1" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
    <table>
        <tr>
            <td colspan="2" align="center">
                Inicio de Sesión
            </td>
        </tr>
        <tr>
            <td align="right">
                Usuario:
            </td>
            <td align="left">
                <asp:TextBox ID="txt_Usuario" runat="server" />
            </td>
        </tr>
        <tr>
            <td align="right">
                Contraseña:
            </td>
            <td align="left">
                <asp:TextBox ID="txt_Contraseña" runat="server" TextMode="Password" />
            </td>
        </tr>
        <tr>
            <td colspan="2" align="right">
                <asp:Button ID="btn_IniciarSesion" runat="server" Text="Enviar" />
            </td>
        </tr>
    </table>
</asp:Content>
